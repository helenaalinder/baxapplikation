<h1>Önska produkter</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= render 'form', comment: @comment %>
  </div>
</div>
<table class="purchases">
	<thead>
		<tr>
			<th>Produkt</th>
			<th>Önskad av</th>
		</tr>
	</thead>

	<tbody>
		<% @comments.each do |c| %>
			<tr>
				<td><%= c.comment %></td>
				<td><%= c.user.name%></td>
			</tr>
		<% end %>
	</tbody>
</table>
<br>
