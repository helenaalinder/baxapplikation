<% provide(:title, 'Sign up') %>
<h1>Ny produkt</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@product) do |f| %>

      <%= f.label :namn %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :pris %>
      <%= f.email_field :price, class: 'form-control' %>

      <%= f.submit "Go!", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>